{
  "manifest_version": 3,
  "name": "Workspace Launcher",
  "description": "Launch a collection of saved sites for different workspaces",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png"
  }
}
